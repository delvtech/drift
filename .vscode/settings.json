{
  "prettier.configPath": ".prettierrc.cjs",
  "[javascript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[json]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "cSpell.words": [
    "delvtech",
    "emptiable",
    "multicall",
    "twoslash",
    "typescale",
    "viem"
  ],
  "typescript.preferences.autoImportFileExcludePatterns": ["**/exports/**"]
}
