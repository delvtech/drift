{
  "name": "@delvtech/drift-web3",
  "description": "Web3.js adapter for Drift",
  "version": "2.0.0-next.0",
  "license": "Apache-2.0",
  "author": "Ryan Goree (https://github.com/ryangoree)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/delvtech/drift.git"
  },
  "homepage": "https://delvtech.github.io/drift",
  "bugs": "https://github.com/delvtech/drift/issues",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/Web3Adapter.d.ts",
        "default": "./dist/Web3Adapter.js"
      },
      "require": {
        "types": "./dist/Web3Adapter.d.cts",
        "default": "./dist/Web3Adapter.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest --reporter=verbose",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch"
  },
  "peerDependencies": {
    "@delvtech/drift": "^2.0.0-next.0",
    "web3": "^4.16.0"
  },
  "devDependencies": {
    "@delvtech/drift": "^2.0.0-next.0",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "web3": "^4.16.0"
  },
  "main": "dist/Web3Adapter.cjs",
  "module": "dist/Web3Adapter.js",
  "types": "dist/Web3Adapter.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/Web3Adapter.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  }
}
