{
  "name": "@delvtech/evm-client-viem",
  "version": "0.0.10",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./stubs": {
      "types": "./dist/stubs.d.ts",
      "default": "./dist/stubs.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "stubs": [
        "./dist/stubs.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "test:watch": "vitest",
    "test": "vitest run",
    "test:integration": "vitest run integration",
    "typecheck": "tsc --noEmit",
    "watch": "tsup --watch"
  },
  "peerDependencies": {
    "sinon": "^17.0.1",
    "viem": "^2"
  },
  "peerDependenciesMeta": {
    "sinon": {
      "optional": true
    }
  },
  "dependencies": {
    "@delvtech/evm-client": "0.0.8"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "sinon": "^17.0.1",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "viem": "^2.7.3",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.2.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ]
}
