{
  "name": "@delvtech/drift-viem",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./testing": {
      "types": "./dist/testing.d.ts",
      "default": "./dist/testing.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      ".": ["./dist/index.d.ts"],
      "testing": ["./dist/testing.d.ts"]
    }
  },
  "scripts": {
    "build": "tsup",
    "test:watch": "vitest",
    "test": "vitest run",
    "test:integration": "vitest run integration",
    "typecheck": "tsc --noEmit",
    "watch": "tsup --watch"
  },
  "peerDependencies": {
    "@delvtech/drift": "0.0.0",
    "sinon": "^17.0.1",
    "viem": "^2"
  },
  "peerDependenciesMeta": {
    "sinon": {
      "optional": true
    }
  },
  "devDependencies": {
    "@delvtech/drift": "0.0.0",
    "@repo/typescript-config": "*",
    "sinon": "^17.0.1",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "viem": "^2.7.3",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.2.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": ["dist"]
}
