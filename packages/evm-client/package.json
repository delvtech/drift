    "dotenv": "^16.4.2",
    "eslint": "^8.56.0",
    "sinon": "^17.0.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
