{
  "name": "example",
  "version": "0.0.0",
  "main": "index.js",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "tsx -r dotenv/config src/index.ts",
    "watch": "tsx -r dotenv/config --watch src/index.ts",
    "sdk": "tsx -r dotenv/config src/sdk.ts",
    "test": "tsx -r dotenv/config --test --test-reporter=spec src/*.test.*",
    "test:watch": "tsx -r dotenv/config --test --test-reporter=spec --watch src/*.test.*"
  },
  "dependencies": {
    "@delvtech/drift": "^0.8.4",
    "@delvtech/fixed-point-wasm": "^0.0.8"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "dotenv": "^16.4.7",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  }
}
